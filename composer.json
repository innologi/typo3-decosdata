{
	"name": "innologi/typo3-decosdata",
	"type": "typo3-cms-extension",
	"description": "Import Decos XML exports locally and flexibly publish their contents. Successor to the 'decospublisher' extension.",
	"license": "GPL-2.0+",
	"authors": [{
			"name": "Frenck Lutke",
			"email": "typo3@innologi.nl",
			"homepage": "http://www.innologi.nl",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=8.2",
		"typo3/cms-core": "^12.4",
		"typo3/cms-scheduler": "^12.4",
		"bmack/modern-template-building": "dev-master#88864b6e82fa78c015a8ea0d18be490782a4f096",
		"friendsoftypo3/typo3db-legacy": "~1.2.0",
		"innologi/tag-builder": "~0.1.4",
		"innologi/trace-logger": "~0.1.1",
		"innologi/typo3-fal-api": "~1.0.0",
		"innologi/typo3-ext-update": "~1.0.0",
		"innologi/typo3-asset-provider": "~1.0.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "decosdata"
		}
	},
	"autoload": {
		"psr-4": {
			"Innologi\\Decosdata\\": "Classes/"
		}
	},
	"replace": {
		"typo3-ter/decosdata": "self.version"
	}
}