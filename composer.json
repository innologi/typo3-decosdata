{
	"name": "innologi/typo3-decosdata",
	"type": "typo3-cms-extension",
	"description": "Import Decos XML exports locally and flexibly publish their contents. Successor to the 'decospublisher' extension.",
	"license": "GPL-2.0+",
	"authors": [{
			"name": "Frenck Lutke",
			"email": "typo3@innologi.nl",
			"homepage": "http://www.innologi.nl",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=8.0",
		"typo3/cms-core": "^10.4 || ^11.0",
		"typo3/cms-scheduler": "^10.4 || ^11.0",
		"bmack/modern-template-building": "^1.0",
		"innologi/tag-builder": "~0.1.1 || dev-develop",
		"innologi/trace-logger": "~0.1.1 || dev-develop",
		"innologi/typo3-fal-api": "~0.2.0 || dev-develop",
		"innologi/typo3-ext-update": "~0.6.0 || dev-develop",
		"innologi/typo3-asset-provider": "~0.4.0 || dev-develop"
	},
	"suggest": {
		"friendsoftypo3/typo3db-legacy": "Required for TYPO3 >=9 support"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "decosdata"
		}
	},
	"autoload": {
		"psr-4": {
			"Innologi\\Decosdata\\": "Classes/"
		}
	},
	"replace": {
		"typo3-ter/decosdata": "self.version"
	}
}